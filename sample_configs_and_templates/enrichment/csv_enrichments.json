        {
          "enrichment_type": "description",
          "config": {
            "description": "The node_utilization_metrics table captures channel-wise utilization and interference information from Access Points (APs) every 15 minutes. It provides channel utilization data on a per-AP, per-channel basis, combining measurement data from both on-channel and off-channel survey reports. This is useful for understanding Wi-Fi channel congestion and interference at a granular level."
          }
        },
        {
          "enrichment_type": "tags",
          "config": {
            "tags": [
              "Wi-Fi Channel Utilization",
              "Interference Analysis",
              "Access Point Monitoring",
              "Mesh Network Analytics",
              "Plume Data Warehouse",
              "Parquet Format",
              "15-minute Aggregation",
              "S3 Data Lake",
              "Adaptive Wi-Fi",
              "Network Health Metrics"
            ]
          }
        },
        {
          "enrichment_type": "documentation",
          "config": {
            "description": "The table is stored in S3 as Parquet files and is updated every 15 minutes. It is part of the Plume Data Warehouse schema and is used for analytics and monitoring of Wi-Fi network health and performance."
          }
        },
        {
          "enrichment_type": "properties",
          "config": {
            "custom_properties": {
              "update_frequency": "15 minutes",
              "storage_format": "Parquet",
              "storage_location": "S3",
              "partitioning": "time, location, node, channel",
              "data_source": "Access Points (APs)",
              "measurement_type": "on-channel and off-channel survey reports"
            }
          }
        }
      ]
    },
