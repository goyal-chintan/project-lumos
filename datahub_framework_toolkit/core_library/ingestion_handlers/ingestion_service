def get_handler(platform):
    if platform == "csv":
        return CsvIngestionHandler()
       
def start_ingestion():
    """
    1. get all the jsons at defined paths: 
    for each folder in the config, go through each jsons
    """
    jsons = []
    for json in jsons:
        platform_handler ph = get_handler(json.get_platform())
        ph.ingest(json)
        
    